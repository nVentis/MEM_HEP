    /nfs/dust/ilc/user/jtorndal/PhysicsAnalysis/slcio/Pe2e2qqh.eL.pR_fullflow_v2.slcio

  -->
  <parameter name="LCIOInputFiles">
    ${InputFiles}
  </parameter>
